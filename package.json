{
  "name": "signalk-parquet",
  "version": "0.5.0-alpha.2",
  "description": "SignalK plugin to save marine data directly to Parquet files with regimen-based control (TypeScript version)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "dev": "npm run build && npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "signalk-node-server-plugin",
    "signalk-webapp",
    "signalk-category-utility",
    "signalk-category-ais",
    "signalk-category-cloud",
    "parquet",
    "data-collection",
    "marine-data",
    "typescript",
    "duckdb",
    "s3",
    "aws"
  ],
  "author": "Maurice Tamman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motamman/zennora-signalk-parquet.git"
  },
  "homepage": "https://github.com/motamman/zennora-signalk-parquet#readme",
  "bugs": {
    "url": "https://github.com/motamman/zennora-signalk-parquet/issues"
  },
  "signalk": {
    "appSupport": "webapp",
    "compatibility": ">=2.13.0",
    "displayName": "SignalK Parquet Data Store",
    "appIcon": "parquet.png"
  },
  "dependencies": {
    "@dsnp/parquetjs": "^1.8.7",
    "fs-extra": "^11.2.0",
    "@duckdb/node-api": "^1.3.1-alpha.23",
    "@aws-sdk/client-s3": "^3.842.0",
    "glob": "^11.0.0",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^11.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/**/*",
    "public/**/*",
    "README.md",
    "LICENSE"
  ]
}